{
    "env": {
    "es2021": true,
    "node": true
    },
    "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
    },
    "plugins": [
    "@typescript-eslint"
    ],
    "rules": {
    "@typescript-eslint/explicit-function-return-type": "off",
    "@typescript-eslint/explicit-module-boundary-types": "off"
    }
}